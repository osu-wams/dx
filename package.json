{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/pro-light-svg-icons": "^5.7.2",
    "@fortawesome/pro-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@material-ui/core": "^4.6.0",
    "@reach/dialog": "^0.6.1",
    "@reach/menu-button": "^0.6.1",
    "@reach/router": "^1.2.1",
    "@reach/visually-hidden": "^0.6.1",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.21",
    "@types/reach__router": "^1.2.6",
    "@types/react": "16.9.12",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.0",
    "axios": "^0.19.0",
    "concurrently": "^4.1.1",
    "date-fns": "^2.8.1",
    "date-fns-tz": "^1.0.8",
    "http-proxy-middleware": "^0.20.0",
    "libphonenumber-js": "^1.7.19",
    "react": "16.12.0",
    "react-app-polyfill": "^1.0.4",
    "react-dom": "16.12.0",
    "react-ga": "^2.7.0",
    "react-helmet-async": "^1.0.4",
    "react-loading-skeleton": "^1.2.0",
    "react-pose": "^4.0.9",
    "react-responsive": "^6.1.1",
    "react-scripts": "^3.2.0",
    "react-swipeable": "^5.4.0",
    "react-test-renderer": "^16.12.0",
    "react-toastify": "5.3.0",
    "reakit": "^1.0.0-beta.12",
    "styled-components": "^4.4.1",
    "styled-map": "^3.3",
    "typescript": "^3.3.3",
    "use-debounce": "^2.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx,.js src/**/*",
    "lint:fix": "eslint --ext .ts,.tsx,.js src/**/* --fix",
    "analyze": "npm run build -- --stats && webpack-bundle-analyzer build/bundle-stats.json"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-pro": "^5.7.2",
    "@testing-library/jest-dom": "4.2.3",
    "@testing-library/react": "9.3.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "axios-mock-adapter": "^1.16.0",
    "codecov": "^3.6.1",
    "cross-env": "^5.2.0",
    "cypress": "^3.4.1",
    "cypress-testing-library": "^4.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^3.1.0",
    "jest-styled-components": "^6.3.4",
    "lint-staged": "^9.2.5",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "jest": {
    "resetMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/registerServiceWorker.js",
      "!src/setupTests.ts",
      "!src/setupProxy.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 30,
        "lines": 30,
        "statements": 10
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run test --all --silent --watchAll=false"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc --fix",
      "git add"
    ]
  }
}
