{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/pro-light-svg-icons": "^5.12.1",
    "@fortawesome/pro-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@material-ui/core": "^4.9.9",
    "@osu-wams/hooks": "^1.0.26",
    "@osu-wams/lib": "^0.1.21",
    "@reach/dialog": "^0.10.3",
    "@reach/menu-button": "^0.10.3",
    "@reach/router": "^1.3.3",
    "@reach/visually-hidden": "^0.10.2",
    "axios": "^0.19.2",
    "date-fns": "^2.13.0",
    "date-fns-tz": "^1.0.10",
    "http-proxy-middleware": "1.0.3",
    "libphonenumber-js": "^1.7.52",
    "react": "16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "16.13.1",
    "react-ga": "^2.7.0",
    "react-helmet-async": "^1.0.4",
    "react-loading-skeleton": "^2.0.1",
    "react-pose": "^4.0.10",
    "react-responsive": "^8.0.3",
    "react-scripts": "^3.4.1",
    "react-swipeable": "^5.5.1",
    "react-toastify": "5.5.0",
    "recoil": "^0.0.7",
    "styled-components": "^5.1.1",
    "styled-map": "^3.3",
    "typescript": "^3.9.3",
    "use-debounce": "^3.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx,.js 'src/**/*'",
    "lint:fix": "eslint --ext .ts,.tsx,.js 'src/**/*' --fix",
    "analyze": "npm run build -- --stats && webpack-bundle-analyzer build/bundle-stats.json"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-pro": "^5.7.2",
    "@sheerun/mutationobserver-shim": "^0.3.3",
    "@testing-library/cypress": "^5.3.0",
    "@testing-library/jest-dom": "5.8.0",
    "@testing-library/react": "10.0.4",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^10.4.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.9.0",
    "@types/reach__router": "^1.3.4",
    "@types/react": "16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "axios-mock-adapter": "^1.18.1",
    "codecov": "^3.6.5",
    "cypress": "^4.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest-dom": "^2.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-testing-library": "^3.1.3",
    "husky": "^4.2.3",
    "jest-styled-components": "^7.0.1",
    "lint-staged": "^10.0.8",
    "react-test-renderer": "^16.13.1",
    "webpack-bundle-analyzer": "^3.6.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "jest": {
    "resetMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/registerServiceWorker.js",
      "!src/setupTests.ts",
      "!src/setupProxy.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 30,
        "lines": 30,
        "statements": 10
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run test --all --silent --watchAll=false"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc --fix"
    ]
  }
}
