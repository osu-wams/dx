{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/pro-light-svg-icons": "^5.12.1",
    "@fortawesome/pro-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.9.9",
    "@osu-wams/hooks": "1.0.126",
    "@osu-wams/lib": "0.1.82",
    "@osu-wams/theme": "0.1.5",
    "@osu-wams/utils": "0.0.17",
    "@reach/accordion": "^0.11.2",
    "@reach/dialog": "^0.11.2",
    "@reach/menu-button": "0.10.5",
    "@reach/router": "^1.3.4",
    "@reach/visually-hidden": "^0.11.1",
    "axios": "^0.21.1",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.10",
    "framer-motion": "^2.9.4",
    "fuse.js": "^6.4.1",
    "http-proxy-middleware": "1.0.5",
    "libphonenumber-js": "^1.9.15",
    "nanoid": "^3.1.23",
    "react": "17.0.2",
    "react-app-polyfill": "2.0.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "17.0.2",
    "react-ga": "^2.7.0",
    "react-helmet-async": "^1.0.6",
    "react-loadable": "^5.5.0",
    "react-loading-skeleton": "^2.1.1",
    "react-query": "3.16.0",
    "react-responsive": "^8.1.0",
    "react-scripts": "^4.0.3",
    "react-swipeable": "^5.5.1",
    "react-toastify": "5.5.0",
    "recoil": "0.2.0",
    "styled-components": "^5.2.2",
    "styled-map": "^3.3",
    "typescript": "^4.2.3",
    "use-debounce": "^3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "TZ=UTC react-scripts test --env=jsdom",
    "test:coverage": "TZ=UTC react-scripts test --env=jsdom --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx,.js 'src/**/*'",
    "lint:css": "stylelint 'src/**/*.tsx'",
    "lint:fix": "eslint --ext .ts,.tsx,.js 'src/**/*' --fix",
    "analyze": "npm run build -- --stats && webpack-bundle-analyzer build/bundle-stats.json"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-pro": "^5.7.2",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "^6.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.20",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^14.14.22",
    "@types/reach__router": "^1.3.7",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-loadable": "^5.5.4",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "axios-mock-adapter": "^1.19.0",
    "codecov": "^3.7.2",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.6.0",
    "husky": "^4.2.5",
    "jest-axe": "^3.5.0",
    "jest-styled-components": "^7.0.4",
    "lint-staged": "^11.0.0",
    "msw": "0.26.0",
    "prettier": "^2.3.0",
    "prop-types": "^15.7.2",
    "react-axe": "^3.5.4",
    "react-test-renderer": "^17.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "jest": {
    "resetMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/registerServiceWorker.js",
      "!src/setupTests.ts",
      "!src/setupProxy.js",
      "!src/util/test-utils.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 30,
        "lines": 30,
        "statements": 10
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run test --all --silent --watchAll=false"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc"
    ]
  }
}
