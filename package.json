{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.14",
    "@fortawesome/pro-light-svg-icons": "^5.7.2",
    "@fortawesome/pro-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@reach/dialog": "^0.2.9",
    "@reach/menu-button": "^0.1.18",
    "@reach/router": "^1.2.1",
    "@reach/visually-hidden": "^0.1.4",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.21",
    "@types/reach__router": "^1.2.3",
    "@types/react": "16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/styled-components": "^4.1.18",
    "axios": "^0.18.0",
    "concurrently": "^4.1.1",
    "contentful": "^7.2.0",
    "date-fns": "^1.30.1",
    "dev": "^0.1.3",
    "http-proxy-middleware": "^0.19.1",
    "libphonenumber-js": "^1.7.19",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-ga": "^2.6.0",
    "react-helmet": "^5.2.1",
    "react-loading-skeleton": "^1.1.2",
    "react-pose": "^4.0.6",
    "react-responsive": "^6.1.1",
    "react-scripts": "^3.1.1",
    "react-swipeable": "^5.0.1",
    "react-test-renderer": "^16.9.0",
    "react-toastify": "5.3.0",
    "reakit": "^1.0.0-beta.6",
    "styled-components": "^4.3.2",
    "styled-map": "^3.3",
    "typescript": "^3.3.3",
    "use-debounce": "^2.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx,.js src/**/*",
    "lint:fix": "eslint --ext .ts,.tsx,.js src/**/* --fix",
    "analyze": "npm run build -- --stats && webpack-bundle-analyzer build/bundle-stats.json"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-pro": "^5.7.2",
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "9.1.4",
    "@testing-library/react-hooks": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "axios-mock-adapter": "^1.16.0",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.4.1",
    "cypress-testing-library": "^4.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^3.0.2",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.2.5",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/registerServiceWorker.js",
      "!src/setupTests.ts",
      "!src/setupProxy.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 30,
        "lines": 30,
        "statements": 10
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc --fix",
      "git add",
      "cross-env CI=true yarn run test --findRelatedTests"
    ]
  }
}
