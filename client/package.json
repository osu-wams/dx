{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.14",
    "@fortawesome/pro-light-svg-icons": "^5.7.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@reach/dialog": "^0.1.2",
    "@reach/menu-button": "^0.1.7",
    "@reach/router": "^1.2.1",
    "@reach/visually-hidden": "^0.1.1",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/reach__router": "^1.2.2",
    "@types/react": "16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/styled-components": "^4.1.6",
    "axios": "^0.18.0",
    "contentful": "^7.1.0",
    "date-fns": "^1.29.0",
    "http-proxy-middleware": "^0.19.1",
    "prop-types": "^15.6.2",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "react-pose": "^4.0.4",
    "react-responsive": "^5.0.0",
    "react-scripts": "^2.1.1",
    "react-toastify": "^4.4.3",
    "styled-components": "^4.0.2",
    "styled-map": "^3.2.0-rc.1",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint \"**/*.js\"",
    "lint:fix": "eslint \"**/*.js\" --fix",
    "analyze": "npm run build -- --stats && webpack-bundle-analyzer build/bundle-stats.json"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "jest-dom": "^2.1.1",
    "jest-styled-components": "^6.3.1",
    "react-testing-library": "^5.3.0",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "**/*.js",
      "!**/coverage/**",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/setupTests.js",
      "!src/componentTestUtils.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 30,
        "lines": 30,
        "statements": -100
      }
    }
  }
}
